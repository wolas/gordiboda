.float_right
  = link_to '', new_admin_user_path, class: "new"

%h1 Listing users

%table
  %tr
    %th.left Name
    %th.left Email
    %th

  - @users.each do |user|
    %tr.clickable{data: {url: url_for([:admin, user])}}
      %td.left= user.name
      %td.left= user.email
      - if user.id == current_user.id
        %td= link_to 'Edit', edit_admin_user_path(current_user.id)


